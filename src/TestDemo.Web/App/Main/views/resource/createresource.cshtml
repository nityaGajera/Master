<form id="frm_create_resourcedocument" name="resourceCreateForm">
    <div class="modal-header">
        <button type="button" class="close" ng-click="vm.cancel()" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        <h4 class="modal-title">
            <span>@("Create Resource Document")</span>
        </h4>
    </div>
    <div class="modal-body">
        @*<span>product Module</span>*@
        <uib-tabset class="tab-container tabbable-line">
            @*<uib-tab heading="product Details">*@
            @*<div class="row clearfix" style="margin-top:10px;">
                <div class="col-sm-12">
                    <div class="form-group form-float">*@
            <div class="form-line">
                <label for="title" class="form-label">@L("Title")</label>
                <input id="title" type="text" name="title" ng-model="vm.resources.title" maxlength="32" minlength="2" class="validate form-control">
                <label for="resource_category" class="form-label">@L("Resource Category")</label>
                <input id="resourcecategory" type="text" name="resourcecategory" ng-model="vm.resources.resourcecategory" maxlength="32" minlength="2" class="validate form-control">

                @*<label for="name" class="form-label">@L("Name")</label>*@
            </div>
    </div>
    </div>
    </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default waves-effect" ng-click="vm.cancel()">
            @L("Cancel")
        </button>
        <button type="submit" class="btn btn-primary waves-effect" ng-click="vm.save()">
            @L("Save")
        </button>
    </div>

</form>